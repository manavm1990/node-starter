{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["xo", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "capitalized-comments": "off",
          "new-cap": "off",
    "no-console": 1,
      "padding-line-between-statements": "warn"
  }
}
